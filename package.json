{
  "name": "ibcp-fileheader",
  "displayName": "ibcp-fileheader",
  "description": "file header",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ibcp-fileheader.fileheader",
        "title": "fileheader"
      }
    ],
    "keybindings": [  
        {  
            "command": "ibcp-fileheader.fileheader",  
            "key": "ctrl+alt+i",  
            "mac": "ctrl+alt+i",
            "when": "editorTextFocus"
        },
        {  
          "command": "ibcp-fileheader.fileheader",  
          "key": "cmd+ctrl+i",
          "mac": "cmd+ctrl+i",
          "when": "editorTextFocus"  
        }
    ]  
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "date-fns": "^3.6.0",
    "ts-node": "^10.9.2"
  }
}
